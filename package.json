{
  "name": "Instagram-Package-Viewer",
  "version": "0.0.2",
  "author": "Felix HH Yeung",
  "homepage": "http://felixhhyeung.ddns.net/",
  "scripts": {
    "ng": "ng",
    "dev": "ionic cap run ios -l --external",
    "build": "ionic build && plutil -replace CFBundleShortVersionString -string ${npm_package_version} ios/App/App/Info.plist && npx cap sync && npx cap open ios",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "describe": "from=$(git describe | cut -d '-' -f1); echo $(tput bold)'Describing from '${from}' to HEAD.'$(tput sgr0); git log --pretty='%h - %s (%an)' ${from}..HEAD"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/forms": "~10.0.0",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/router": "~10.0.0",
    "@capacitor/core": "2.4.2",
    "@capacitor/ios": "2.4.2",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/file": "^5.29.0",
    "@ionic-native/file-transfer": "^5.29.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic-native/video-editor": "^5.29.0",
    "@ionic-native/zip": "^5.29.0",
    "@ionic/angular": "^5.0.0",
    "@types/axios": "^0.14.0",
    "axios": "^0.21.0",
    "bluebird": "^3.7.2",
    "cordova-plugin-file": "^6.0.2",
    "cordova-plugin-file-transfer": "^1.7.1",
    "cordova-plugin-video-editor": "^1.1.3",
    "cordova-plugin-zip": "^3.1.0",
    "rxjs": "~6.5.5",
    "ts-md5": "^1.2.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "~10.0.5",
    "@angular/compiler": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
